
@{
    Layout = null;
}
<style>
    hr {
        margin-top: 0;
    }

    tr, td, th {
        white-space: nowrap
    }
</style>

<div class="fullDiv" ng-if="dataLoading">
    <div class="processDiv">
        <i class="icon-spinner2 spinner" style="font-size:6rem;color:#178076"></i>
    </div>
</div>

<div id="myModal" class="Modal is-hidden is-visuallyHidden" ng-if="dataLoading">
</div>

<div class="page-header page-header-light" ng-cloak>
    <div class="breadcrumb-line breadcrumb-line-light header-elements-md-inline">
        <div class="d-flex">
            <div class="breadcrumb">
                <a href="#!" class="breadcrumb-item"><i class="icon-home2 mr-2"></i> Home</a>
                <span class="breadcrumb-item">Examination</span>
                <span class="breadcrumb-item active">Admint Card Print</span>
            </div>
            <a href="#" class="header-elements-toggle text-default d-md-none"><i class="icon-more"></i></a>
        </div>
    </div>
</div>

<div class="content" ng-cloak>
    <div class="card">
        <div class="card-header bg-white header-elements-inline">
            <h6 class="card-title">Admint Card Print</h6>
        </div>
        <div class="card-body">
            <form name="formValidation" novalidate>
                <div class="form-group row">
                    <div class="col-lg-4">
                        <label class="col-form-label font-weight-bold col-lg-4">Session <span class="text-danger">*</span></label>
                        <div class="col-lg-8">
                            <select ng-model="AcademicSessionId" name="AcademicSessionId" ng-change="getClassList()||getSectionList()" ng-options="option.AcademicSessionId as option.SessionName for option in AcademicSessionList" class="form-control" ng-class="IsSubmitted ? 'ng-dirty' : ''" required autofocus>
                                <option value="">--Select Session--</option>
                            </select>
                            <span class="text-danger" ng-if="(formValidation.AcademicSessionId.$dirty || IsSubmitted) && formValidation.AcademicSessionId.$error.required">Session is required!</span>
                        </div>
                    </div>
                    <div class="col-lg-4">
                        <label class="col-form-label font-weight-bold col-lg-4">Class <span class="text-danger">*</span></label>
                        <div class="col-lg-8">
                            <select ng-model="ClassNo" name="ClassNo" ng-change="changeClass()" ng-options="option.ClassNo as option.ClassName for option in ClassList" class="form-control" ng-class="IsSubmitted ? 'ng-dirty' : ''" required autofocus>
                                <option value="">--Select Class--</option>
                            </select>
                            <span class="text-danger" ng-if="(formValidation.ClassNo.$dirty || IsSubmitted) && formValidation.ClassNo.$error.required">Class is required!</span>
                        </div>
                    </div>
                    <div class="col-lg-4">
                        <label class="col-form-label font-weight-bold col-lg-4">Section <span class="text-danger">*</span></label>
                        <div class="col-lg-8">
                            <select ng-model="SectionId" name="SectionId" ng-options="option.SectionId as option.SectionName for option in SectionList" class="form-control" ng-class="IsSubmitted ? 'ng-dirty' : ''" required autofocus>
                                <option value="">--Select Section--</option>
                            </select>
                             <span class="text-danger" ng-if="(formValidation.SectionId.$dirty || IsSubmitted) && formValidation.SectionId.$error.required">Section is required!</span>
                        </div>
                    </div>
                </div>
                <div class="form-group row">
                    @*<div class="col-lg-4">
                            <label class="col-form-label font-weight-bold col-lg-4">Term <span class="text-danger">*</span></label>
                            <div class="col-lg-8">
                                <select ng-model="ExamTerm" name="ExamTerm" ng-options="option.Key as option.Value for option in ExamTermList" class="form-control" ng-class="IsSubmitted ? 'ng-dirty' : ''" required autofocus>
                                    <option value="">--Select Term--</option>
                                </select>
                                <span class="text-danger" ng-if="(formValidation.ExamTerm.$dirty || IsSubmitted) && formValidation.ExamTerm.$error.required">Term is required!</span>
                            </div>
                        </div>*@

                    <div class="col-lg-4">
                        <label class="col-form-label font-weight-bold col-lg-4">Examination <span class="text-danger">*</span></label>
                        <div class="col-lg-8">
                            <select ng-model="ExaminationId" name="ExaminationId" ng-options="option.ExaminationId as option.ExaminationName for option in ExaminationList" class="form-control" ng-class="IsSubmitted ? 'ng-dirty' : ''" required autofocus>
                                <option value="">--Select Examination--</option>
                            </select>
                            <span class="text-danger" ng-if="(formValidation.ExaminationId.$dirty || IsSubmitted) && formValidation.ExaminationId.$error.required">Examination is required!</span>
                        </div>
                    </div>
                </div>

            </form>

            <div class="col-lg-12 form-group text-center" style="padding-top:20px">
                <button class="btn btn-sm btn-success" ng-click="printReport('PrintAdmitCard')"> Print</button>
            </div>
        </div>
    </div>
</div>
