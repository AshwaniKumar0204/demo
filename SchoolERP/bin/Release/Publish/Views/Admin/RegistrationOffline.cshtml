@{
    Layout = null;
}

<div class="fullDiv" ng-if="dataLoading">
    <div class="processDiv">
        <i class="icon-spinner2 spinner" style="font-size:6rem;color:#178076"></i>
    </div>
</div>

<div id="myModal" class="Modal is-hidden is-visuallyHidden" ng-if="dataLoading">
</div>

<div class="page-header page-header-light" ng-cloak>
    <div class="breadcrumb-line breadcrumb-line-light header-elements-md-inline">
        <div class="d-flex">
            <div class="breadcrumb">
                <a href="#!" class="breadcrumb-item"><i class="icon-home2 mr-2"></i> Home</a>
                <a class="breadcrumb-item"> Registration Online</a>
                <span class="breadcrumb-item active">New Registration</span>
            </div>
            <a href="#" class="header-elements-toggle text-default d-md-none"><i class="icon-more"></i></a>
        </div>
    </div>
</div>

<div class="content" ng-cloak>
    <div class="card">
        <div class="card-header bg-white header-elements-inline">
            <h6 class="card-title">New Registration</h6>
        </div>
        <div class="card-body">
            <form name="formRegistrationOnline" novalidate>
                <div class="row">
                    <div class="col-md-6 form-group">
                        <label class="col-form-label font-weight-bold col-lg-4">Select Course <span class="text-danger">*</span></label>
                        <div class="col-sm-8">
                            <select ng-model="RegistrationOnline.ClassNo" name="ClassNo" ng-options="option.ClassNo as option.ClassName for option in ClassList" class="form-control" ng-class="(RegistrationOnline.ClassNo == null && IsSubmitted) ? 'ng-pristine ng-dirty ng-empty ng-invalid ng-invalid-required ng-touched' : ''">
                                <option value="">--Select Course--</option>
                            </select>
                            <span class="text-danger" ng-if="(RegistrationOnline.ClassNo == null && IsSubmitted)">Course is required!</span>
                        </div>
                    </div>
                    <div class="col-md-6 form-group" ng-show="RegistrationForm.ClassNo == 1">
                        <label class="col-form-label font-weight-bold col-lg-4">Select Entry Type <span class="text-danger">*</span></label>
                        <div class="col-sm-8">
                            <select ng-model="RegistrationOnline.EntryType" name="EntryType" ng-options="option.Key as option.Value for option in EntryTypeList" class="form-control" ng-class="(RegistrationOnline.EntryType == null && IsSubmitted) ? 'ng-pristine ng-dirty ng-empty ng-invalid ng-invalid-required ng-touched' : ''">
                                <option value="">--Select Entry Type--</option>
                            </select>
                            <span class="text-danger" ng-if="(RegistrationOnline.EntryType == null && IsSubmitted)">Entry Type is required!</span>
                        </div>
                    </div>
                    <div class="col-md-6 form-group">
                        <label class="col-form-label font-weight-bold col-lg-4">{{RegistrationForm.ClassNo == 4 ? 'Branch' :'Choice 1'}}  <span class="text-danger">*</span></label>
                        <div class="col-sm-8">
                            <select ng-model="RegistrationOnline.SectionOne" ng-change="changeSectionOne()" name="SectionOne" ng-options="option.SectionId as option.SectionName for option in SectionList" class="form-control" ng-class="IsSubmitted ? 'ng-dirty' : ''" required autofocus>
                                <option value="">--Select {{RegistrationForm.ClassNo == 4 ? 'Branch' :'Choice 1'}}--</option>
                            </select>
                            <span class="text-danger" ng-if="(formRegistrationOnline.SectionOne.$dirty || IsSubmitted) && formRegistrationOnline.SectionOne.$error.required">{{RegistrationForm.ClassNo == 4 ? 'Branch' :'Choice 1'}} is required!</span>
                            @*<span class="text-danger" ng-if="(RegistrationOnline.SectionOne == null && IsSubmitted)">Choice 1 is required!</span>*@
                        </div>
                    </div>
                    <div class="col-md-6 form-group" ng-show="RegistrationOnline.SectionOne && RegistrationForm.ClassNo == 1">
                        <label class="col-form-label font-weight-bold col-lg-4">Choice 2 <span class="text-danger">*</span></label>
                        <div class="col-sm-8">
                            <select ng-model="RegistrationOnline.SectionTwo" ng-change="changeSectionTwo()" name="SectionTwo" ng-options="option.SectionId as option.SectionName for option in SectionTwoList" class="form-control" ng-class="(RegistrationOnline.SectionTwo == null && IsSubmitted) ? 'ng-pristine ng-dirty ng-empty ng-invalid ng-invalid-required ng-touched' : ''">
                                <option value="">--Select Choice 2--</option>
                            </select>
                            <span class="text-danger" ng-if="(RegistrationOnline.SectionTwo == null && IsSubmitted)">Choice 2 is required!</span>
                        </div>
                    </div>
                    <div class="col-md-6 form-group" ng-show="RegistrationOnline.SectionTwo && RegistrationForm.ClassNo == 1">
                        <label class="col-form-label font-weight-bold col-lg-4">Choice 3 <span class="text-danger">*</span></label>
                        <div class="col-sm-8">
                            <select ng-model="RegistrationOnline.SectionThree" ng-change="changeSectionThree()" name="SectionThree" ng-options="option.SectionId as option.SectionName for option in SectionThreeList" class="form-control" ng-class="(RegistrationOnline.SectionThree == null && IsSubmitted) ? 'ng-pristine ng-dirty ng-empty ng-invalid ng-invalid-required ng-touched' : ''">
                                <option value="">--Select Choice 2--</option>
                            </select>
                            <span class="text-danger" ng-if="(RegistrationOnline.SectionThree == null && IsSubmitted)">Choice 3 is required!</span>
                        </div>
                    </div>
                    <div class="col-md-6 form-group" ng-show="RegistrationOnline.SectionThree && RegistrationForm.ClassNo == 1">
                        <label class="col-form-label font-weight-bold col-lg-4">Choice 4</label>
                        <div class="col-sm-8">
                            <select ng-model="RegistrationOnline.SectionFour" ng-change="changeSectionFour()" name="SectionFour" ng-options="option.SectionId as option.SectionName for option in SectionFourList" class="form-control">
                                <option value="">--Select Choice 2--</option>
                            </select>
                        </div>
                    </div>
                    <div class="col-md-6 form-group" ng-show="RegistrationOnline.SectionFour && RegistrationForm.ClassNo == 1">
                        <label class="col-form-label font-weight-bold col-lg-4">Choice 5 </label>
                        <div class="col-sm-8">
                            <select ng-model="RegistrationOnline.SectionFive" name="SectionFive" ng-options="option.SectionId as option.SectionName for option in SectionFiveList" class="form-control">
                                <option value="">--Select Choice 2--</option>
                            </select>
                        </div>
                    </div>
                    @*<div class="col-md-6 form-group">
            <label class="col-form-label font-weight-bold col-lg-4">Select Entry Type <span class="text-danger">*</span></label>
            <div class="col-sm-8">
                <select ng-model="RegistrationOnline.EntryType" name="EntryType" ng-options="option.Key as option.Value for option in EntryTypeList" class="form-control" ng-class="IsSubmitted ? 'ng-dirty' : ''" required autofocus>
                    <option value="">--Select Entry Type--</option>
                </select>
                <span class="text-danger" ng-if="(formRegistrationOnline.EntryType.$dirty || IsSubmitted) && formRegistrationOnline.EntryType.$error.required">Entry Type is required!</span>
            </div>
        </div>*@
                    <div class="col-md-6 form-group">
                        <label class="col-form-label font-weight-bold col-lg-4">Candidate Full Name <span class="text-danger">*</span></label>
                        <div class="col-sm-8">
                            <input type="text" name="StudentName" placeholder="Enter Candidate Full Name" ng-model="RegistrationOnline.StudentName" ng-class="IsSubmitted ? 'ng-dirty' : ''" class="form-control required" required autofocus>
                            <span class="text-danger" ng-if="(formRegistrationOnline.StudentName.$dirty || IsSubmitted) && formRegistrationOnline.StudentName.$error.required">Full Name is required!</span>
                        </div>
                    </div>
                    <div class="col-md-6 form-group">
                        <label class="col-form-label font-weight-bold col-lg-4">Select Gender <span class="text-danger">*</span></label>
                        <div class="col-sm-8">
                            <select ng-model="RegistrationOnline.Gender" name="Gender" ng-options="option.Key as option.Value for option in GenderList" class="form-control" ng-class="IsSubmitted ? 'ng-dirty' : ''" required autofocus>
                                <option value="">--Select Gender--</option>
                            </select>
                            <span class="text-danger" ng-if="(formRegistrationOnline.Gender.$dirty || IsSubmitted) && formRegistrationOnline.Gender.$error.required">Gender is required!</span>
                        </div>
                    </div>
                    <div class="col-md-6 form-group">
                        <label class="col-form-label font-weight-bold col-lg-4">DOB <span class="text-danger">*</span></label>
                        <div class="col-sm-8">
                            <input type="date" name="DOB" placeholder="Enter DOB" ng-model="RegistrationOnline.DOB" ng-class="IsSubmitted ? 'ng-dirty' : ''" class="form-control required" required autofocus>
                            <span class="text-danger" ng-if="(formRegistrationOnline.DOB.$dirty || IsSubmitted) && formRegistrationOnline.DOB.$error.required">DOB is required!</span>
                        </div>
                    </div>
                    <div class="col-md-6 form-group">
                        <label class="col-form-label font-weight-bold col-lg-4">Father's Name <span class="text-danger">*</span></label>
                        <div class="col-sm-8">
                            <input type="text" name="FathersName" placeholder="Enter Father's Name" ng-model="RegistrationOnline.FathersName" ng-class="IsSubmitted ? 'ng-dirty' : ''" class="form-control required" required autofocus>
                            <span class="text-danger" ng-if="(formRegistrationOnline.FathersName.$dirty || IsSubmitted) && formRegistrationOnline.FathersName.$error.required">Fathers Name is required!</span>
                        </div>
                    </div>
                    <div class="col-md-6 form-group">
                        <label class="col-form-label font-weight-bold col-lg-4">Mother's Name <span class="text-danger">*</span></label>
                        <div class="col-sm-8">
                            <input type="text" name="MothersName" placeholder="Enter Mother's Name" ng-model="RegistrationOnline.MothersName" ng-class="IsSubmitted ? 'ng-dirty' : ''" class="form-control required" required autofocus>
                            <span class="text-danger" ng-if="(formRegistrationOnline.MothersName.$dirty || IsSubmitted) && formRegistrationOnline.MothersName.$error.required">Mother's Name is required!</span>
                        </div>
                    </div>
                    <div class="col-md-6 form-group">
                        <label class="col-form-label font-weight-bold col-lg-4">Address <span class="text-danger">*</span></label>
                        <div class="col-sm-8">
                            <textarea name="CorrespondenceAddress" placeholder="Enter Address" ng-model="RegistrationOnline.CorrespondenceAddress" ng-class="IsSubmitted ? 'ng-dirty' : ''" class="form-control required" required autofocus></textarea>
                            <span class="text-danger" ng-if="(formRegistrationOnline.CorrespondenceAddress.$dirty || IsSubmitted) && formRegistrationOnline.CorrespondenceAddress.$error.required">Address is required!</span>
                        </div>
                    </div>

                    <div class="col-md-6 form-group">
                        <label class="col-form-label font-weight-bold col-lg-4">State <span class="text-danger">*</span></label>
                        <div class="col-sm-8">
                            <select ng-model="RegistrationOnline.StateId" ng-change="changeState()" name="State" ng-options="option.StateId as option.StateName for option in StateList" class="form-control" ng-class="IsSubmitted ? 'ng-dirty' : ''" required autofocus>
                                <option value="">--Select State--</option>
                            </select>
                            <span class="text-danger" ng-if="(formRegistrationOnline.State.$dirty || IsSubmitted) && formRegistrationOnline.State.$error.required">State is required!</span>
                        </div>
                    </div>

                    <div class="col-md-6 form-group">
                        <label class="col-form-label font-weight-bold col-lg-4">City <span class="text-danger">*</span></label>
                        <div class="col-sm-8">
                            <select ng-model="RegistrationOnline.CityId" name="City" ng-options="option.CityId as option.CityName for option in CityList" class="form-control" ng-class="IsSubmitted ? 'ng-dirty' : ''" required autofocus>
                                <option value="">--Select City--</option>
                            </select>
                            <span class="text-danger" ng-if="(formRegistrationOnline.City.$dirty || IsSubmitted) && formRegistrationOnline.City.$error.required">City is required!</span>
                        </div>
                    </div>

                    <div class="col-md-6 form-group">
                        <label class="col-form-label font-weight-bold col-lg-4">Pin Code</label>
                        <div class="col-sm-8">
                            <input type="number" name="PinCode" placeholder="Enter Pin Code" ng-model="RegistrationOnline.PinCode" class="form-control">
                        </div>
                    </div>

                    <div class="col-md-6 form-group">
                        <label class="col-form-label font-weight-bold col-lg-4">Phone No @*<span class="text-danger">*</span>*@</label>
                        <div class="col-sm-8">
                            <input type="number" name="PhoneNo" placeholder="Enter Phone No  with STD Code" ng-model="RegistrationOnline.PhoneNo" class="form-control" @*ng-class="IsSubmitted ? 'ng-dirty' : ''" required autofocus*@>
                            @* <span class="text-danger" ng-if="(formRegistrationOnline.PhoneNo.$dirty || IsSubmitted) && formRegistrationOnline.PhoneNo.$error.required">Phone No is required!</span>*@
                        </div>
                    </div>

                    <div class="col-md-6 form-group">
                        <label class="col-form-label font-weight-bold col-lg-4">Mobile No <span class="text-danger">*</span></label>
                        <div class="col-sm-8">
                            <input type="number" name="MobileNo" placeholder="Enter Mobile No" ng-model="RegistrationOnline.MobileNo" ng-class="IsSubmitted ? 'ng-dirty' : ''" class="form-control required" required autofocus>
                            <span class="text-danger" ng-if="(formRegistrationOnline.MobileNo.$dirty || IsSubmitted) && formRegistrationOnline.MobileNo.$error.required">Mobile No is required!</span>
                        </div>
                    </div>
                    <div class="col-md-6 form-group">
                        <label class="col-form-label font-weight-bold col-lg-4">Email Id <span class="text-danger">*</span></label>
                        <div class="col-sm-8">
                            <input type="email" name="EmailId" placeholder="Enter Email Id" ng-model="RegistrationOnline.EmailId" ng-class="IsSubmitted ? 'ng-dirty' : ''" class="form-control required" required autofocus>
                            <span class="text-danger" ng-if="(formRegistrationOnline.EmailId.$dirty || IsSubmitted) && formRegistrationOnline.EmailId.$error.required">Email Id is required!</span>
                            <span class="text-danger" ng-if="formRegistrationOnline.$error.email">Invalid Email</span>
                        </div>
                    </div>
                    <div class="col-md-6 form-group">
                        <label class="col-form-label font-weight-bold col-lg-4">Blood Group</label>
                        <div class="col-sm-8">
                            <input type="text" name="BloodGroup" placeholder="Enter Blood Group" ng-model="RegistrationOnline.BloodGroup" class="form-control">
                        </div>
                    </div>
                    <div class="col-md-6 form-group">
                        <label class="col-form-label font-weight-bold col-lg-4">Nationality <span class="text-danger">*</span></label>
                        <div class="col-sm-8">
                            <select ng-model="RegistrationOnline.Nationality" name="Nationality" ng-options="option.Key as option.Value for option in NationalityList" class="form-control" ng-class="IsSubmitted ? 'ng-dirty' : ''" required autofocus>
                                <option value="">--Select Nationality--</option>
                            </select>
                            <span class="text-danger" ng-if="(formRegistrationOnline.Nationality.$dirty || IsSubmitted) && formRegistrationOnline.Nationality.$error.required">Nationality is required!</span>
                        </div>
                    </div>
                    <div class="col-md-6 form-group">
                        <label class="col-form-label font-weight-bold col-lg-4">Category <span class="text-danger">*</span></label>
                        <div class="col-sm-8">
                            <select ng-model="RegistrationOnline.Category" name="Category" ng-options="option.Key as option.Value for option in CategoryList" class="form-control" ng-class="IsSubmitted ? 'ng-dirty' : ''" required autofocus>
                                <option value="">--Select Category--</option>
                            </select>
                            <span class="text-danger" ng-if="(formRegistrationOnline.Category.$dirty || IsSubmitted) && formRegistrationOnline.Category.$error.required">Category is required!</span>
                        </div>
                    </div>
                    <div class="col-md-6 form-group">
                        <label class="col-form-label font-weight-bold col-lg-4">10th Board <span class="text-danger">*</span></label>
                        <div class="col-sm-8">
                            <input type="text" name="MetricBoardName" placeholder="Enter 10th Board" ng-model="RegistrationOnline.MetricBoardName" ng-class="IsSubmitted ? 'ng-dirty' : ''" class="form-control required" required autofocus>
                            <span class="text-danger" ng-if="(formRegistrationOnline.MetricBoardName.$dirty || IsSubmitted) && formRegistrationOnline.MetricBoardName.$error.required">10th Board is required!</span>
                        </div>
                    </div>

                    <div class="col-md-6 form-group">
                        <label class="col-form-label font-weight-bold col-lg-4">10th Passing Year <span class="text-danger">*</span></label>
                        <div class="col-sm-8">
                            <input type="number" name="MetricPassingYear" placeholder="Enter 10th Passing Year" ng-model="RegistrationOnline.MetricPassingYear" ng-class="IsSubmitted ? 'ng-dirty' : ''" class="form-control required" required autofocus>
                            <span class="text-danger" ng-if="(formRegistrationOnline.MetricPassingYear.$dirty || IsSubmitted) && formRegistrationOnline.MetricPassingYear.$error.required">10th Passing Year is required!</span>
                        </div>
                    </div>

                    <div class="col-md-6 form-group">
                        <label class="col-form-label font-weight-bold col-lg-4">10th Percentage Marks <span class="text-danger">*</span></label>
                        <div class="col-sm-8">
                            <input type="number" max="100" name="MetricPercentage" placeholder="Enter 10th Percentage Marks" ng-model="RegistrationOnline.MetricPercentage" ng-class="IsSubmitted ? 'ng-dirty' : ''" class="form-control required" required autofocus>
                            <span class="text-danger" ng-if="(formRegistrationOnline.MetricPercentage.$dirty || IsSubmitted) && formRegistrationOnline.MetricPercentage.$error.required">10th Percentage Marks is required!</span>
                            <span class="text-danger" ng-if="formRegistrationOnline.MetricPercentage.$error.max">Invalid Value!</span>
                        </div>
                    </div>

                    <div class="col-md-6 form-group">
                        <label class="col-form-label font-weight-bold col-lg-4">{{RegistrationForm.ClassNo == 4 ? 'Graduation ' : (RegistrationOnline.EntryType == 2 ? 'Diploma ' : 'Inter ')}} Passing Status<span class="text-danger">*</span></label>
                        <div class="col-sm-8">
                            <select ng-model="RegistrationOnline.InterPassingStatus" name="InterPassingStatus" ng-options="option.Key as option.Value for option in PassingStatusList" class="form-control" ng-class="IsSubmitted ? 'ng-dirty' : ''" required autofocus>
                                <option value="">--Select Passing Status --</option>
                            </select>
                            <span class="text-danger" ng-if="(formRegistrationOnline.InterPassingStatus.$dirty || IsSubmitted) && formRegistrationOnline.InterPassingStatus.$error.required">{{RegistrationForm.ClassNo == 4 ? 'Graduation ' : 'Inter '}} Passing Status is required!</span>
                        </div>
                    </div>
                    <div class="col-md-6 form-group">
                        <label class="col-form-label font-weight-bold col-lg-4">{{RegistrationForm.ClassNo == 4 ? 'Board/University ' : 'Board '}} Name<span class="text-danger">*</span></label>
                        <div class="col-sm-8">
                            <input type="text" name="BoardOrUniversity" placeholder="Enter {{RegistrationForm.ClassNo == 4 ? 'Board/University ' : 'Board '}} Name" ng-model="RegistrationOnline.BoardOrUniversity" ng-class="IsSubmitted ? 'ng-dirty' : ''" class="form-control required" required autofocus>
                            <span class="text-danger" ng-if="(formRegistrationOnline.BoardOrUniversity.$dirty || IsSubmitted) && formRegistrationOnline.BoardOrUniversity.$error.required">{{RegistrationForm.ClassNo == 4 ? 'Board/University ' : 'Board '}} Name is required!</span>
                        </div>
                    </div>
                    <div class="col-md-4 form-group" ng-show="RegistrationOnline.InterPassingStatus == 2">
                        <label class="col-form-label font-weight-bold col-lg-6">Passing Year <span class="text-danger">*</span></label>
                        <div class="col-sm-6">
                            <input type="number" name="PassingYear" placeholder="Enter Passing Year" ng-model="RegistrationOnline.PassingYear" ng-class="(RegistrationOnline.PassingYear == null && IsSubmitted) ? 'ng-pristine ng-dirty ng-empty ng-invalid ng-invalid-required ng-touched' : ''" class="form-control required">
                            <span class="text-danger" ng-if="(RegistrationOnline.PassingYear == null && IsSubmitted)">Passing Year is required!</span>
                        </div>
                    </div>

                    <div class="col-md-8 form-group" ng-show="RegistrationOnline.InterPassingStatus == 2 && RegistrationOnline.ClassNo != 4 && RegistrationOnline.EntryType == 1">
                        <label class="col-form-label font-weight-bold col-lg-3">PCM Marks <span class="text-danger">*</span></label>
                        <div class="col-sm-3">
                            <input type="number" max="100" ng-change="changeInterMarks()" name="PhysicsMarks" placeholder="Enter Physics Marks" ng-model="RegistrationOnline.PhysicsMarks" class="form-control" ng-class="((RegistrationOnline.PhysicsMarks == null && IsSubmitted) || formRegistrationOnline.PhysicsMarks.$error.max) ? 'ng-pristine ng-dirty ng-empty ng-invalid ng-invalid-required ng-touched' : ''">
                            <span class="text-danger" ng-if="(RegistrationOnline.PhysicsMarks == null && IsSubmitted)">Physics Marks is required!</span>
                            <span class="text-danger" ng-if="formRegistrationOnline.PhysicsMarks.$error.max">Invalid Value!</span>
                        </div>
                        <div class="col-sm-3">
                            <input type="number" max="100" ng-change="changeInterMarks()" name="ChemistryMarks" placeholder="Enter Chemistry Marks" ng-model="RegistrationOnline.ChemistryMarks" class="form-control required" ng-class="((RegistrationOnline.ChemistryMarks == null && IsSubmitted) || formRegistrationOnline.ChemistryMarks.$error.max) ? 'ng-pristine ng-dirty ng-empty ng-invalid ng-invalid-required ng-touched' : ''">
                            <span class="text-danger" ng-if="(RegistrationOnline.ChemistryMarks == null && IsSubmitted)">Chemistry Marks is required!</span>
                            <span class="text-danger" ng-if="formRegistrationOnline.ChemistryMarks.$error.max">Invalid Value!</span>
                        </div>
                        <div class="col-sm-3">
                            <input type="number" ng-change="changeInterMarks()" max="100" name="MathMarks" placeholder="Enter Math Marks" ng-model="RegistrationOnline.MathMarks" class="form-control required" ng-class="((RegistrationOnline.MathMarks == null && IsSubmitted) || formRegistrationOnline.MathMarks.$error.max)? 'ng-pristine ng-dirty ng-empty ng-invalid ng-invalid-required ng-touched' : ''">
                            <span class="text-danger" ng-if="(RegistrationOnline.MathMarks == null && IsSubmitted)">Math Marks is required!</span>
                            <span class="text-danger" ng-if="formRegistrationOnline.MathMarks.$error.max">Invalid Value!</span>
                        </div>
                    </div>

                    <div class="col-md-6 form-group" ng-show="RegistrationOnline.InterPassingStatus == 2 && RegistrationOnline.EntryType == 1">
                        <label class="col-form-label font-weight-bold col-lg-4">PCM Percentage <span class="text-danger">*</span></label>
                        <div class="col-sm-8">
                            <input type="number" disabled name="PCMPercentage" placeholder="Enter PCM Percentage" ng-model="RegistrationOnline.PCMPercentage" class="form-control required" ng-class="((RegistrationOnline.PCMPercentage == null && IsSubmitted)) ? 'ng-pristine ng-dirty ng-empty ng-invalid ng-invalid-required ng-touched' : ''">
                            <span class="text-danger" ng-if="(RegistrationOnline.PCMPercentage == null && IsSubmitted)">PCM Percentage is required!</span>
                        </div>
                    </div>

                    <div class="col-md-6 form-group" ng-show="RegistrationForm.ClassNo == 4 || RegistrationOnline.EntryType == 2">
                        <label class="col-form-label font-weight-bold col-lg-4">Stream ({{RegistrationForm.ClassNo == 4 ?'PG':'Diploma'}})  <span class="text-danger">*</span></label>
                        <div class="col-sm-8">
                            <input type="text" name="SubjectStream" placeholder="Enter Stream ({{RegistrationForm.ClassNo == 4 ?'PG':'Diploma'}}) " ng-model="RegistrationOnline.SubjectStream" class="form-control required" ng-class="(RegistrationOnline.SubjectStream == null && IsSubmitted) ? 'ng-pristine ng-dirty ng-empty ng-invalid ng-invalid-required ng-touched' : ''">
                            <span class="text-danger" ng-if="(RegistrationOnline.SubjectStream == null && IsSubmitted)">Stream ({{RegistrationForm.ClassNo == 4 ?'PG':'Diploma'}})  is required!</span>
                        </div>
                    </div>

                    <div class="col-md-6 form-group" ng-show="((RegistrationForm.ClassNo == 4) || (RegistrationOnline.EntryType == 2))">
                        <label class="col-form-label font-weight-bold col-lg-4">{{RegistrationOnline.InterPassingStatus == 1 ? '5th Semester ' :(RegistrationForm.ClassNo == 4 ? 'Graduation Total ' : 'Diploma Total ')}} Percentage <span class="text-danger">*</span></label>
                        <div class="col-sm-8">
                            <input type="number" name="TotalPercentage" max="100" placeholder="Enter {{RegistrationOnline.InterPassingStatus == 1 ? '5th Semester ' :(RegistrationForm.ClassNo == 4 ? 'Graduation Total ' : 'Diploma Total ')}} Percentage" ng-model="RegistrationOnline.TotalPercentage" class="form-control required" ng-class="((RegistrationOnline.TotalPercentage == null && IsSubmitted) || formRegistrationOnline.TotalPercentage.$error.max) ? 'ng-pristine ng-dirty ng-empty ng-invalid ng-invalid-required ng-touched' : ''">
                            <span class="text-danger" ng-if="(RegistrationOnline.TotalPercentage == null && IsSubmitted)">{{RegistrationOnline.InterPassingStatus == 1 ? '5th Semester ' :(RegistrationForm.ClassNo == 4 ? 'Graduation Total ' : 'Diploma Total ')}} Percentage is required!</span>
                            <span class="text-danger" ng-if="formRegistrationOnline.TotalPercentage.$error.max">Invalid Value!</span>
                        </div>
                    </div>
                </div>
            </form>
        </div>
        <div class="card-footer text-centre">
            <button class="btn btn-primary" ng-click="saveRegistrationOnline()">Update</button>
        </div>
    </div>
</div>

