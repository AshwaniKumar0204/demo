                                <th ng-if="IsMediumOfInstruction" style="cursor: pointer;" ng-click="sort('Student.MediumOfInstruction')">Medium Of Instruction <span class="fa" ng-if="sortKey=='Student.MediumOfInstruction'" ng-class="{'fa-arrow-up':reverse,'fa-arrow-down':!reverse}"></span></th>
                                <th ng-if="IsPreviousSchoolTCNo" style="cursor: pointer;" ng-click="sort('Student.PreviousSchoolTCNo')">Previous School TC No <span class="fa" ng-if="sortKey=='Student.PreviousSchoolTCNo'" ng-class="{'fa-arrow-up':reverse,'fa-arrow-down':!reverse}"></span></th>
                                <th ng-if="IsPreviousSchoolTCIssueDate" style="cursor: pointer;" ng-click="sort('Student.PreviousSchoolTCIssueDate')">TC Issue Date <span class="fa" ng-if="sortKey=='Student.PreviousSchoolTCIssueDate'" ng-class="{'fa-arrow-up':reverse,'fa-arrow-down':!reverse}"></span></th>
                                <th ng-if="IsHeight" style="cursor: pointer;" ng-click="sort('Student.Height')">Height <span class="fa" ng-if="sortKey=='Student.Height'" ng-class="{'fa-arrow-up':reverse,'fa-arrow-down':!reverse}"></span></th>
                                <th ng-if="IsWeight" style="cursor: pointer;" ng-click="sort('Student.Weight')">Weight <span class="fa" ng-if="sortKey=='Student.Weight'" ng-class="{'fa-arrow-up':reverse,'fa-arrow-down':!reverse}"></span></th>
                                <th ng-if="IsJoiningDate" style="cursor: pointer;" ng-click="sort('Student.JoiningDate')">Join Date <span class="fa" ng-if="sortKey=='Student.JoiningDate'" ng-class="{'fa-arrow-up':reverse,'fa-arrow-down':!reverse}"></span></th>
                                <th ng-if="IsRemarks" style="cursor: pointer;" ng-click="sort('Student.Remarks')">Remarks <span class="fa" ng-if="sortKey=='Student.Remarks'" ng-class="{'fa-arrow-up':reverse,'fa-arrow-down':!reverse}"></span></th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr dir-paginate="admission in AdmissionList|orderBy:sortKey:reverse|filter:search|itemsPerPage:pageSize" current-page="currentPage" ng-model="search">
                                <td ng-if="IsSLNo">{{(currentPage*pageSize - (pageSize-1))+ $index}}</td>
                                <td ng-if="IsAdmissionNo">{{admission.Student.AdmissionNo}}</td>
                                <td ng-if="IsFullName">{{admission.Student.FullName}}</td>
                                <td ng-if="IsSessionName">{{admission.SessionName}}</td>
                                <td ng-if="IsClassName">{{admission.ClassName}}</td>
                                <td ng-if="IsSectionName">{{admission.SectionName}}</td>
                                <td ng-if="IsRollNo">{{admission.RollNo}}</td>
                                <td ng-if="IsDateOfBirth">{{admission.Student.DateOfBirth | date:'dd MMM, yyyy'}}</td>
                                <td ng-if="IsGender">{{GenderList[admission.Student.Gender].Value}}</td>
                                <td ng-if="IsFatherName">{{admission.Student.FatherName}}</td>
                                <td ng-if="IsMotherName">{{admission.Student.MotherName}}</td>
                                <td ng-if="IsFatherOccupation">{{admission.Student.FatherOccupation}}</td>
                                <td ng-if="IsFatherAadhaarNo">{{admission.Student.FatherAadhaarNo}}</td>
                                <td ng-if="IsCategory">{{CategoryList[admission.Student.Category].Value}}</td>
                                <td ng-if="IsBloodGroup">
                                    <span ng-if="admission.Student.BloodGroup != 0">{{BloodGroupList[admission.Student.BloodGroup].Value}}</span>
                                    <span ng-if="admission.Student.BloodGroup == 0">N/A</span>
                                </td>
                                <td ng-if="IsHouseName">{{admission.Student.HouseName}}</td>
                                <td ng-if="IsCorrespondenceAddressLine1">{{admission.Student.CorrespondenceAddressLine1}}</td>
                                <td ng-if="IsCorrespondenceAddressLine2">{{admission.Student.CorrespondenceAddressLine2}}</td>
                                <td ng-if="IsCorrespondenceCityName">{{admission.Student.CorrespondenceCityName}}</td>
                                <td ng-if="IsCorrespondenceStateName">{{admission.Student.CorrespondenceStateName}}</td>
                                <td ng-if="IsCorrespondecePinCode">{{admission.Student.CorrespondecePinCode}}</td>
                                <td ng-if="IsPermanentAddressLine1">{{admission.Student.PermanentAddressLine1}}</td>
                                <td ng-if="IsPermanentAddressLine2">{{admission.Student.PermanentAddressLine2}}</td>
                                <td ng-if="IsPermanentCityName">{{admission.Student.PermanentCityName}}</td>
                                <td ng-if="IsPermanentStateName">{{admission.Student.PermanentStateName}}</td>
                                <td ng-if="IsPermanentPinCode">{{admission.Student.PermanentPinCode}}</td>
                                <td ng-if="IsMobileNo">{{admission.Student.MobileNo}}</td>
                                <td ng-if="IsAlternateNo">{{admission.Student.AlternateNo}}</td>
                                <td ng-if="IsEmail">{{admission.Student.Email}}</td>
                                <td ng-if="IsAadhaarNo">{{admission.Student.AadhaarNo}}</td>
                                <td ng-if="IsFamilyAnnualIncome">{{admission.Student.FamilyAnnualIncome | number:2}}</td>
                                <td ng-if="IsAdmissionDate">{{admission.AdmissionDate | date:'dd MMM, yyyy'}}</td>
                                <td ng-if="IsPreviousSchoolName">{{admission.Student.PreviousSchoolName}}</td>
                                <td ng-if="IsPreviousBoardName">{{admission.Student.PreviousBoardName}}</td>
                                <td ng-if="IsLastClassAttend">{{admission.Student.LastClassAttend}}</td>
                                <td ng-if="IsMediumOfInstruction">{{admission.Student.MediumOfInstruction}}</td>
                                <td ng-if="IsPreviousSchoolTCNo">{{admission.Student.PreviousSchoolTCNo}}</td>
                                <td ng-if="IsPreviousSchoolTCIssueDate">{{admission.Student.PreviousSchoolTCIssueDate | date:'dd MMM, yyyy'}}</td>
                                <td ng-if="IsHeight">{{admission.Student.Height}}</td>
                                <td ng-if="IsWeight">{{admission.Student.Weight}}</td>
                                <td ng-if="IsJoiningDate">{{admission.Student.JoiningDate | date:'dd-MM-yyyy'}}</td>
                                <td ng-if="IsRemarks">{{admission.Student.Remarks}}</td>
                            </tr>
                        </tbody>
                    </table>
                </div>
                <div class="clearfix">
                    <dir-pagination-controls max-size="pageSize" boundary-links="true" template-url="/Scripts/DirPagination/dirPagination.tpl.html"></dir-pagination-controls>
                </div>
            </fieldset>

        </div>
    </div>
</div>

